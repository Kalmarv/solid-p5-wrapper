{
  "version": "1.0.3",
  "name": "solid-p5-wrapper",
  "description": "A solidjs wrapper for p5.js",
  "license": "MIT",
  "author": "Kalmarv",
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kalmarv/solid-p5-wrapper.git"
  },
  "homepage": "https://github.com/Kalmarv/solid-p5-wrapper#readme",
  "bugs": {
    "url": "https://github.com/Kalmarv/solid-p5-wrapper/issues"
  },
  "private": false,
  "sideEffects": false,
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "solid": "./dist/source/index.jsx",
      "import": "./dist/esm/index.js",
      "browser": {
        "import": "./dist/esm/index.js",
        "require": "./dist/cjs/index.js"
      },
      "require": "./dist/cjs/index.js",
      "node": "./dist/cjs/index.js"
    }
  },
  "scripts": {
    "dev": "vite serve dev",
    "test": "echo \"test not setup\"",
    "build": "rollup -c",
    "prepublishOnly": "pnpm build",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "solid-js": ">=1.0.0"
  },
  "devDependencies": {
    "@kalmarv/prettier-config": "^1.0.2",
    "@types/p5": "^1.4.2",
    "prettier": "2.7.1",
    "rollup": "^2.77.2",
    "rollup-preset-solid": "^1.4.0",
    "solid-js": "^1.4.8",
    "typescript": "^4.7.4",
    "vite": "^3.0.4",
    "vite-plugin-solid": "^2.3.0"
  },
  "keywords": [
    "solid"
  ],
  "dependencies": {
    "p5": "^1.4.2"
  }
}
